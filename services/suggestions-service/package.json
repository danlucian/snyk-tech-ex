{
  "name": "suggestions-service",
  "version": "0.0.1",
  "description": "Service that acts as a proxy against NPM Registry search",
  "scripts": {
    "watch": "tsc --watch --preserveWatchOutput --pretty",
    "dev": "cross-env NODE_ENV=development nodemon",
    "format": "prettier --write \"src/**/*.ts\"",
    "build": "rimraf dist && tsc",
    "start": "node dist/index.js",
    "test": "TS_NODE_CACHE=false TS_NODE_PROJECT=tsconfig.json tap --ts --no-timeout --reporter=spec test/**/*.ts",
    "test-debug": "DEBUG=nock.* TS_NODE_CACHE=false TS_NODE_PROJECT=tsconfig.json tap --ts --no-timeout --reporter=spec test/**/*.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danlucian/snyk-tech-ex.git"
  },
  "keywords": [
    "snyk",
    "npmjs",
    "interview"
  ],
  "author": "Lucian Rosu",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/danlucian/snyk-tech-ex/issues"
  },
  "homepage": "https://github.com/danlucian/snyk-tech-ex#readme",
  "dependencies": {
    "axios": "^0.21.1",
    "cross-env": "^7.0.3",
    "fastify": "^3.20.2",
    "pino-elasticsearch": "^6.1.0",
    "pino-multi-stream": "^5.3.0",
    "retry-axios": "^2.6.0"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/jsonschema": "^1.1.1",
    "@types/nock": "^11.1.0",
    "@types/node": "^16.7.8",
    "@types/pino": "^6.3.11",
    "@types/pino-multi-stream": "^5.1.2",
    "@types/supertest": "^2.0.11",
    "@types/tap": "^15.0.5",
    "fastify-tsconfig": "^1.0.1",
    "jsonschema": "^1.4.0",
    "nock": "^13.1.3",
    "nodemon": "^2.0.12",
    "prettier": "^2.3.2",
    "rimraf": "^3.0.2",
    "supertest": "^6.1.6",
    "tap": "^15.0.9",
    "ts-node": "^10.2.1",
    "tsconfig-paths": "^3.11.0",
    "typescript": "^4.4.2"
  }
}
